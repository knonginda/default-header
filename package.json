{
  "name": "@knonginda/default-header",
  "version": "1.0.5",
  "description": "default header",
  "main": "build/index.common.js",
  "scripts": {
    "dev": "vue-cli-service serve --open",
    "build": "vue-cli-service build --target lib --name index ./src/components/_globals.js",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:all:eslint": "npm run lint:eslint \"src/**/*.{vue,js}\"",
    "lint:all:stylelint": "npm run lint:stylelint \"src/**/*.{vue, scss}\"",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{json,css,scss,html,md}\"",
    "lint": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:ci": "npm run test:unit --coverage --ci",
    "test:unit:file": "npm run test:unit --bail --findRelatedTests",
    "test": "run-s test:unit"
  },
  "dependencies": {
    "lodash": "4.17.11",
    "vue": "2.6.10"
  },
  "devDependencies": {
    "@knonginda/base": "1.0.14",
    "@vue/cli-plugin-babel": "3.5.1",
    "@vue/cli-plugin-eslint": "3.5.1",
    "@vue/cli-plugin-unit-jest": "3.5.1",
    "@vue/cli-service": "3.5.1",
    "@vue/eslint-config-prettier": "4.0.1",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "eslint": "5.15.3",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "5.2.2",
    "express": "4.16.4",
    "lint-staged": "8.1.5",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "sass-loader": "7.1.0",
    "stylelint": "9.10.1",
    "stylelint-config-css-modules": "1.3.0",
    "stylelint-config-prettier": "5.0.0",
    "stylelint-config-recess-order": "2.0.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-scss": "3.5.4",
    "vue-eslint-parser": "6.0.3",
    "vue-jest": "3.0.2",
    "vue-template-compiler": "2.6.10"
  },
  "files": [
    "build/index.common.js",
    "build/index.css"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "author": "Jett Liu",
  "license": "ISC",
  "keywords": [
    "vuejs",
    "default-header"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
